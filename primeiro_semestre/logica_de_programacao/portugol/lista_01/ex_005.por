/*Escreva um algoritmo que leia o nome e sobrenome do usuário e exiba o nome completo
na mesma linha.*/
programa {
  funcao inicio() {
    cadeia nome, sobrenome
    
    escreva("Informe seu nome: ")
    leia(nome)
    escreva("Informe seu sobrenome: ")
    leia(sobrenome)
    limpa()

    escreva("Olá, ", nome, " ", sobrenome, "!")

  }
// Autor: umsimplesrodrigo
}
